<templates>

  <!-- java -->

  <codetemplate description="New classes and interfaces" name="newtype" context="codetemplate"
>for (int ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	${cursor}
}</codetemplate>

  <codetemplate description="Code in new catch blocks" name="catchblock" context="codetemplate"
>for (int ${index} = 0; ${index} &lt; ${array}.length; ${index}++) {
	${array_type} ${array_element} = ${array}[${index}];
	${cursor}
}</codetemplate>

  <codetemplate description="Code in new method bodies" name="methodbody" context="codetemplate"
>for (Iterator ${iterator} = ${collection}.iterator(); ${iterator}.hasNext(); ) {
	${type} ${element} = (${type}) ${iterator}.next();
	${cursor}
}</codetemplate>


</templates>
