<?xml version="1.0"?>
<plugin
    name = "Java Development Tools Example Projects"
    id = "org.eclipse.jdt.ui.examples.projects"
    version = "0.9"
    provider-name = "Object Technology International, Inc."
    class="org.eclipse.jdt.internal.ui.exampleprojects.ExampleProjectsPlugin">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
   </requires>

   <runtime>
      <library name="jdtexamples.jar"/>
   </runtime>
  
 <extension 
	point="org.eclipse.ui.newWizards">
	<category
	   id="org.eclipse.jdt.ui.examples.projects"
	   name="Java Example Project">
	</category>
	<wizard 
		id="org.eclipse.jdt.examples.projects.JUnitProjectExampleCreationWizard"
		name="JUnit"
		class="org.eclipse.jdt.internal.ui.exampleprojects.ExampleProjectCreationWizard"
		category="org.eclipse.jdt.ui.examples.projects"
		project="true"
		finalPerspective="org.eclipse.jdt.ui.JavaPerspective"
		icon="icons/full/ctool16/newjprjex_wiz.gif">
		<description>Create a new JUnit project</description>
		<pagetitle>JUnit</pagetitle>
		<pagedescription>Create a Java project containing the JUnit source</pagedescription>
		<projectsetup output="" jre="true">
    		<src path="" import="archive/junit/junit37src.jar"/>
    	</projectsetup>
      </wizard>	
</extension>  
</plugin>
